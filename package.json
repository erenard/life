{
  "name": "conway-life",
  "version": "1.0.0",
  "description": "Conway game of life",
  "main": "main.js",
  "scripts": {
    "build": "webpack --env production",
    "electron": "electron .",
    "start": "webpack-dev-server --env development",
    "lint": "eslint . --cache",
    "clean": "del ./dist",
    "test": "mocha-webpack --webpack-config webpack.config.test.js \"test/**/*.spec.js\"",
    "cover": "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erenard/life.js.git"
  },
  "keywords": [
    "conway",
    "game",
    "of",
    "life"
  ],
  "author": "Eric Renard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erenard/life.js/issues"
  },
  "homepage": "https://github.com/erenard/life.js#readme",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-preset-env": "^1.2.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.26.1",
    "electron": "^1.4",
    "eslint": "^3.12.2",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "inject-loader": "^3.0.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "mocha-webpack": "^0.7.0",
    "nyc": "^10.2.0",
    "remap-istanbul": "^0.9.5",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^1.16.2",
    "webpack-mocha-plugin": "^0.2.1",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "pixi.js": "^4.4.4",
    "stats.js": "^0.17.0"
  },
  "nyc": {
    "include": [
      "app/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false
  }
