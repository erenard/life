{
  "name": "conway-life",
  "version": "1.0.0",
  "description": "Conway game of life",
  "main": "main.js",
  "scripts": {
    "build": "webpack --env production",
    "electron": "electron .",
    "start": "webpack-dev-server --env development",
    "lint": "eslint . --cache --fix",
    "clean": "del ./dist",
    "test": "mocha-webpack --webpack-config webpack.config.test.js \"test/**/*.spec.js\"",
    "cover": "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text npm run test",
    "coveralls": "cross-env NODE_ENV=coverage nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erenard/life.js.git"
  },
  "keywords": [
    "conway",
    "game",
    "of",
    "life"
  ],
  "author": "Eric Renard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erenard/life.js/issues"
  },
  "homepage": "https://github.com/erenard/life.js#readme",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^7",
    "babel-preset-env": "^1.2.1",
    "chai": "^4",
    "coveralls": "^3",
    "cross-env": "^5",
    "css-loader": "^0.28",
    "eslint": "^4",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsdoc": "^3.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^1",
    "html-webpack-plugin": "^2.28.0",
    "inject-loader": "^3.0.0",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "mocha": "^4",
    "mocha-webpack": "^1",
    "nyc": "^11",
    "remap-istanbul": "^0.9.5",
    "sinon": "^4",
    "style-loader": "^0.19.0",
    "vue-loader": "^13",
    "vue-template-compiler": "^2.3.3",
    "webpack": "^3",
    "webpack-dev-server": "^2.4.5",
    "webpack-mocha-plugin": "^0.2.1",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "electron": "^1",
    "pixi.js": "^4.4.4",
    "stats.js": "^0.17.0",
    "vue": "^2.3.3"
  },
  "nyc": {
    "include": [
      "app/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false
  }
}
