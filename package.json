{
  "name": "conway-life",
  "version": "1.0.0",
  "description": "Conway game of life",
  "main": "app/index.js",
  "scripts": {
    "build": "webpack --config config/webpack.config.js --mode production",
    "electron": "electron app/electron.js",
    "start": "webpack-dev-server --config config/webpack.config.js --mode development",
    "lint": "eslint . --ext .js,.vue --cache --fix",
    "prepare": "git clone https://github.com/erenard/life.js.git --branch gh-pages dist",
    "deploy": "cd dist && git add --all && git commit -m \"Release at $(date)\" && git push",
    "test": "karma start config/karma.conf.js --single-run",
    "test:start": "karma start config/karma.conf.js",
    "bundle-analyzer": "webpack --config config/webpack.config.js --mode production --profile --bundle-analyzer"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erenard/life.js.git"
  },
  "keywords": [
    "conway",
    "game",
    "of",
    "life"
  ],
  "author": "Eric Renard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erenard/life.js/issues"
  },
  "homepage": "https://github.com/erenard/life.js#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsdoc": "^8.0.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.2",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "puppeteer": "^1.17.0",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.33.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.7.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "pixi.js": "^4.8.5",
    "stats.js": "^0.17.0",
    "vue": "^2.6.10"
  }
