{
  "name": "conway-life",
  "version": "1.0.0",
  "description": "Conway game of life",
  "main": "app/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "electron": "electron app/electron.js",
    "start": "webpack-dev-server --mode development",
    "lint": "eslint . --cache --fix",
    "prepare": "git clone https://github.com/erenard/life.js.git --branch gh-pages dist",
    "deploy": "cd dist && git add --all && git commit -m \"Release at $(date)\" && git push",
    "test": "webpack --mode development --config webpack.config.test.js && mocha ./.tmp/tests.js",
    "cover": "cross-env NODE_ENV=coverage nyc --reporter=lcov --reporter=text npm run test",
    "coveralls": "cross-env NODE_ENV=coverage nyc npm test && nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/erenard/life.js.git"
  },
  "keywords": [
    "conway",
    "game",
    "of",
    "life"
  ],
  "author": "Eric Renard",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/erenard/life.js/issues"
  },
  "homepage": "https://github.com/erenard/life.js#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "7",
    "babel-preset-env": "^1.2.1",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsdoc": "^7.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^4.0.0",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "istanbul": "^0.4.5",
    "istanbul-instrumenter-loader": "^3.0.0",
    "json-loader": "^0.5.4",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "sinon": "^7.3.2",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "vue-loader": "^15.7.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-dev-server": "^3.6.0",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "normalize.css": "^8.0.1",
    "pixi.js": "^4.8.5",
    "stats.js": "^0.17.0",
    "vue": "^2.6.10"
  },
  "nyc": {
    "include": [
      "app/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false
  }
}
